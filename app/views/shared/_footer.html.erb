<div class="footer">
  <%=link_to browse_index_path do %>
    <%# <i class="fa-house" ></i> %>
    <i class="fa-solid fa-house"></i>
  <% end %>
  <%= link_to matches_path do=%>
    <i class="fa-regular fa-comment"></i></i>
  <% end %>
  <i class="fa-regular fa-calendar fa-xl"></i>
  <% if current_user != nil %>
    <%= link_to user_path(current_user) do %>
      <%= image_tag "handsome man.png", class:"avatar" %>
    <% end %>
  <% else %>
    <%= image_tag "empty_user.jpg", class:"avatar" %>
  <% end %>
</div>
