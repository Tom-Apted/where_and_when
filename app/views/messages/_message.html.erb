<div class="message-container <%= message.user == current_user ? "received" : "sent" %>">
        <div id="message-<%= message.id %>">
        <small>
            <strong><%= message.user.first_name %></strong>
            <i><%= message.created_at.strftime("%b %e %l:%M %p") %></i>
        </small>
        <p><%= message.content %></p>
        </div>
</div>